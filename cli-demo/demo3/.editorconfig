root = true

[*.js]
charset = utf-8
indent_style = tab
indent_size = tab
tab_width = 2
insert_final_newline = true
trim_trailing_whitespace = true
curly_bracket_next_line = false
spaces_around_operators = true
quote_type = single

[*.html]
charset = utf-8
indent_style = tab
indent_size = tab
tab_width = 2
